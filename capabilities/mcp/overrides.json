{
  "entries": [
    {
      "matches": [
        "github.*",
        "mcp.github.*",
        "github.search_code"
      ],
      "server": {
        "$schema": null,
        "name": "github/github-mcp",
        "description": "Official GitHub MCP Server (requires local npm installation - stdio transport)",
        "version": "1.0.0",
        "repository": {
          "url": "https://github.com/github/github-mcp",
          "source": "github"
        },
        "packages": [
          {
            "registryType": "npm",
            "identifier": "@github/github-mcp",
            "version": "^1.0.0",
            "registryBaseUrl": "https://registry.npmjs.org",
            "runtimeHint": "node",
            "transport": {
              "type": "stdio"
            },
            "environmentVariables": [
              {
                "name": "GITHUB_TOKEN",
                "description": null,
                "isSecret": true,
                "isRequired": true,
                "format": null,
                "default": null
              }
            ]
          }
        ],
        "remotes": [
          {
            "type": "http",
            "url": "https://api.githubcopilot.com/mcp/",
            "headers": [
              {
                "name": "Authorization",
                "description": "GitHub Personal Access Token for MCP authentication",
                "value": null,
                "isSecret": true,
                "isRequired": true
              }
            ]
          }
        ]
      }
    }
  ]
}