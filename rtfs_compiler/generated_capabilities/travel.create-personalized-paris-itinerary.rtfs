(capability "travel.create-personalized-paris-itinerary"
  :description "Create a personalized trip itinerary for Paris based on user preferences and constraints"
  :parameters {:travel-dates "string" :budget "string" :interests "list" :accommodation-type "string"}
  :implementation (do
    (search.flights :destination "Paris" :dates travel-dates)
    (search.hotels :destination "Paris" :dates travel-dates :type accommodation-type)
    (search.activities :destination "Paris" :interests interests)
    (optimize.itinerary :constraints {:budget budget :dates travel-dates})
  )
)